import java.util.Scanner;
public class Demo{
	int a;
	int b;
	Demo(int a,int b){
		this.a=a;
		this.b=b;
	}
	int add(){
		return a+b;
	}
	int subtract(){
		return a-b;
	}
	public static void main(String[]args){
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter a: ");
		int a=sc.nextInt();
		System.out.print("Enter b: ");
		int b=sc.nextInt();
		Demo d=new Demo(a,b);
		System.out.println("Addition: "+d.add());
		System.out.println("Subtraction: "+d.subtract());
	}		
}	
