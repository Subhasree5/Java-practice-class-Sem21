interface Showable{
	void show();
	interface Message{
		void msg();
	}
}
class B implements Showable{
	public void show(){
		System.out.println("Show");
	}
	public void msg(){
		System.out.println("Message");
	}
}
public class NestedInterface{
	public static void main(String []args){
		B obj=new B();
		obj.show();
		obj.msg();
	}
}	
output
Show
Message
