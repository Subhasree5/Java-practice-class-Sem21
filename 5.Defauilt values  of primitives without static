public class Defaultvalues{
	boolean defaultBoolean;
	char defaultChar;
	byte defaultByte;
	short defaultShort;
	int defaultInt;
	long defaultLong;
	float defaultFloat;
	double defaultDouble;
	void display(){
		System.out.println("Boolean: "+defaultBoolean);
		System.out.println("Char: "+defaultChar);
		System.out.println("Byte: "+defaultByte);
		System.out.println("Short: "+defaultShort);
		System.out.println("Int: "+defaultInt);
		System.out.println("Long: "+defaultLong);
		System.out.println("Float: "+defaultFloat);
		System.out.println("Double: "+defaultDouble);
	}
	public static void main(String[]args){
		Defaultvalues defaults=new Defaultvalues();
		defaults.display();
	}	
}

output:
Boolean: false
Char: 
Byte: 0
Short: 0
Int: 0
Long: 0
Float: 0.0
Double: 0.0
